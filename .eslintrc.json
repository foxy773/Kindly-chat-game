{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-classes-per-file": "off",
        "indent": ["warn", 4],
        "jsx-quotes": ["warn", "prefer-double"],
        "quotes": ["error", "double"],
        "no-param-reassign":"off",
        "max-len": ["warn", { "code": 120 }],
        "no-use-before-define": "warn",
        "consistent-return": "warn",
        "import/extensions": "warn"
    }
}
